generator client { 
  provider = "prisma-client-js" 
} 
 
datasource db { 
  provider = "sqlite" 
  url      = env("DATABASE_URL") 
} 
 
model Property { 
  id          String   @id @default(cuid()) 
  title       String 
  description String? 
  price       Float 
  address     String 
  city        String 
  state       String 
  zipCode     String? 
  bedrooms    Int? 
  bathrooms   Int? 
  area        Float? 
  createdAt   DateTime @default(now()) 
  updatedAt   DateTime @updatedAt 
} 
